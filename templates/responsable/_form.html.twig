{{ form_start(form) }}
 <div class="form-row">
    <div class="form-group col-md-6">
      {{ form_row(form.nom) }}
    </div>
    <div class="form-group col-md-6">
       {{ form_row(form.prenom) }}
    </div>
     <div class="form-group col-md-6">
       {{ form_row(form.tel) }}
    </div>
    <div class="form-group col-md-6">
       {{ form_row(form.email) }}
    </div>
    <div class="form-group col-md-6">
       {{ form_row(form.proffession) }}
    </div>
    <div class="form-group col-md-6">
       {{ form_row(form.adresse) }}
    </div>
  </div>

   
    <br>
    <button class="btn btn-success">{{ button_label|default('Enregister ') }}</button>
{{ form_end(form) }}
